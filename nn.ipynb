    "from torch.utils.tensorboard import SummaryWriter\n",
    "\n",
    "# 创建一个SummaryWriter对象\n",
    "writer = SummaryWriter(\"logs\")\n",
    "for i in range(100):\n",
    "    writer.add_scalar(\"y=x\", i, i)\n",
    "\n",
    "writer.close()"
